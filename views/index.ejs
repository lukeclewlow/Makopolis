  <!-- Added socket js file and script for connecting (only) locally-->
  <script src="/socket.io/socket.io.js"></script>
  <script>
    var socket = io();
    var playerID;
    
    socket.on('newPlayer', function (data) {
      console.log(data);
    });

    socket.on('playerID', function(obj) {
      console.log("I am: "+obj.data);
      playerID = obj.data;
    });

  </script>
